
<% include ../partials/header %>


	


<div class="container">
	<h1 class="ui header">
		Top Users in the App:
	</h1>



	<ul class="list-group">
		<% users.forEach(function(user,index){ %>
		  <li class="list-group-item d-flex justify-content-between align-items-center">

		  		<div>
		  			
			  		<img src="<%= user.img%>" style="max-height: 50px;border-radius:50% ">
			    	<a style = "color:black; text-decoration: none" href="/users/<%user._id%>"><%= index + 1 %> - <%=user.username%></a>
			    	<span class="badge badge-primary"><%=user.totalScore%></span>
		  			
		  		</div>
		  		

		  		
		  </li>

		<% }); %>
		  
	</ul>




	
</div>

<% include ../partials/footer %>
